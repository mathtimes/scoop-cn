{
    "description": "The IBM Semeru Runtimes are free production-ready binaries built with the OpenJDK class libraries and the Eclipse OpenJ9 JVM",
    "homepage": "https://developer.ibm.com/languages/java/semeru-runtimes/",
    "version": "11.0.13-8-0.29.0",
    "license": "GPL-2.0-only WITH Classpath-exception-2.0,EPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://download.fastgit.org/ibmruntimes/semeru11-binaries/releases/download/jdk-11.0.13+8_openj9-0.29.0/ibm-semeru-open-jdk_x64_windows_11.0.13_8_openj9-0.29.0.zip",
            "hash": "600a2aa710854c6dbc6e82bc3a59f6ca702b18b58b95d4681207014c885776c2"
        }
    },
    "extract_dir": "jdk-11.0.13+8",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "github": "https://github.com/ibmruntimes/semeru11-binaries",
        "regex": "(?<tag>jdk-(?<major>(?<jdk>[\\d]+)[\\d.]+)(?:\\%2B|\\+)(?<build>[\\d]+)(?<patch>[\\d.]*)(?:(?<oj>_openj9?)(?<jvmver>-[\\d.]+))?)",
        "replace": "${major}-${build}${patch}${jvmver}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.fastgit.org/ibmruntimes/semeru11-binaries/releases/download/$matchTag/ibm-semeru-open-jdk_x64_windows_$matchMajor_$matchBuild$matchPatch_openj9$matchJvmver.zip"
            }
        },
        "hash": {
            "url": "$url.sha256.txt",
            "find": "^([a-fA-F0-9]+)\\s"
        },
        "extract_dir": "jdk-$matchMajor+$matchBuild"
    }
}
